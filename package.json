    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "jsdom": "^12.0.0"
