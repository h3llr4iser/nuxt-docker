{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" : [
        {
            "name": "client-web",
            "image": "h3llr4iser/nuxt-web",
            "hostname": "web",
            "essential": false,
            "memory": 128
            
        },
        {
            "name": "nginx",
            "image": "h3llr4iser/nuxt-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
               {
                   "hostPort": "80",
                   "containerPort": "80"
               } 
            ],
            "links": ["client-web"],
            "memory": 128
        }
    ]
}